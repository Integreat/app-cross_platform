<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:pages="clr-namespace:Integreat.Shared.Pages"
             x:Class="Integreat.Shared.Pages.TabPage" Title="TabPage" BarTextColor="{StaticResource accentColor}"
            Children="{Binding Childs}">
  <TabbedPage.ToolbarItems>
    <ToolbarItem Command="{Binding ChangeLanguageCommand}"  Order="Primary" Priority="0" Icon="Globe.png"/>
    <ToolbarItem Command="{Binding OpenSearchCommand}"  Order="Primary" Priority="0" Icon="Search.png"/>
  </TabbedPage.ToolbarItems>
  
</TabbedPage>