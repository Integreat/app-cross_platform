<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="Integreat.Shared.Views.FeedbackView">
    <ContentView.Content>
        <StackLayout
            Orientation="Vertical">
            <Label
            Text="Feedback"    
            TextColor="{StaticResource TextColor}" />
            <StackLayout
                Orientation="Horizontal">
                <Image
                    Source="smile"/>
                <Image
                    Source="frown"/>
            </StackLayout>
        </StackLayout>
    </ContentView.Content>
</ContentView>
