	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
  	x:Class="Integreat.Shared.IntegreatApp">
  <Application.Resources>
    <ResourceDictionary>
      <ImageSourceConverter x:Key="imageSourceConverter" />
